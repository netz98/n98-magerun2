{
    "name": "n98/magerun2",
    "description": "Tools for managing Magento projects and installations",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/netz98/n98-magerun2/issues"
    },
    "homepage": "http://netz98.github.com/n98-magerun2/",
    "keywords": [
        "magento",
        "installer",
        "cli",
        "magerun2"
    ],
    "require": {
        "php": "^7.0.0",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-PDO": "*",
        "ext-posix": "*",
        "ext-SimpleXML": "*",
        "ext-zip": "*",
        "composer/composer": "~1.4.1",
        "fzaninotto/faker": "~1.4",
        "n98/junit-xml": "~1.0",
        "psy/psysh": "~0.7",
        "symfony/console": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "symfony/finder": "~2.3||~3.0",
        "symfony/process": "~2.3",
        "symfony/validator": "~2.3",
        "symfony/yaml": "~2.3",
        "twig/twig": "~1.0",
        "pbergman/tree-helper": "^1.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "bamarni/symfony-console-autocomplete": "^1.2.0",
        "friendsofphp/php-cs-fixer": "~1.12.0",
        "mikey179/vfsStream": "~1.4",
        "phing/phing": "~2.10.0",
        "phpunit/phpunit": "~6.2.0",
        "seld/phar-utils": "~1.0.1"
    },
    "suggest": {
         "n98/magerun2-dist": "Deploys only the phar file. Use this to prevent dependency conflicts."
    }, 
    "autoload": {
        "psr-0": {
            "N98": "src"
        },
        "psr-4": {
            "N98\\": "shared/src/N98"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "N98": "tests"
        }
    },
    "authors": [
        {
            "name": "Christian MÃ¼nch",
            "email": "c.muench@netz98.de"
        }
    ],
    "bin": [
        "bin/n98-magerun2"
    ],
    "config": {
        "platform": {
            "php": "7.0.0"
        },
        "use-include-path": false
    }
}
